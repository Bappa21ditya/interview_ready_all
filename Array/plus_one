vector<int> Solution::plusOne(vector<int> &A) {
   int carry=1;
   reverse(A.begin(),A.end());
//    vector<int>res;
   int n=A.size();
    for(int i=0;i<n;i++)
    {
      int sum=A[i]+carry;
    //   res.push_back(sum%10);
    A[i]=sum%10;
      carry=sum/10;
         
    }
   if(carry==1)
    {
       A.push_back(carry);
      carry=0;
    }
      reverse(A.begin(),A.end());
      while(A[0]==0){
       A.erase(A.begin());
      }
    return A;

}
