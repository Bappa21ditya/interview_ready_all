  
  1 2 3 4 5
  1+1+1+1=4
  
  2-1=1;
  3-2=1;
  4-3=1;
  5-4=1;
  
  [7,1,5,3,6,4]
  
  7>1
  5-1=4
  5>3
  5-3=3;
  6>4
  
  4+3=7
  
  int dif=0;
  for(int i=0;i<nums.size();i++)
  {
  if(nums[i]>nums[i-1]
     diff+=nums[i]-nums[i-1]
  }
  return diff;
  
  
  int profit=0;
        for(int i=1;i<nums.size();i++)
        {
            if(nums[i]-nums[i-1]>0)
                profit+=nums[i]-nums[i-1];
        }
        return profit;
